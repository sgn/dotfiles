[SpamFilter]
[KillThreadsFilter]
[ListMailsFilter]
[SentMailsFilter]
sent_tag = sent

[FolderNameFilter]
folder_explicit_list = archives

[Filter.1]
query=to:/danh/ or from:/danh/
tags = +me

[InboxFilter]

[MailMover]
folders = Inbox trash
rename = false
Inbox = 'tag:deleted':trash 'tag:archives':archives
trash = 'NOT tag:deleted':archives
