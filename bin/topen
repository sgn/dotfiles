#!/bin/sh

if ! command -v tmux >/dev/null 2>&1; then
	exec xdg-open "$@"
fi


for browser in w3m elinks lynx elinks; do
	command -v $browser >/dev/null 2>&1 &&
		BROWSER=$browser &&
		export BROWSER &&
		break
done; unset browser

if tmux has-session; then
	exec tmux new-window $BROWSER "$@"
else
	exec tmux new-session $BROWSER "$@"
fi
